{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": false,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "module": "commonjs",
    "target": "ES2022",
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": [
    "*.test.ts"
  ]
}
